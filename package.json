{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/src/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "set ENV=test&& db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "tsc": "tsc",
    "prettier": "prettier --config .prettierrc --write src/**/*.ts && prettier --config .prettierrc --write spec/tests/**/*.ts",
    "lint": "eslint src/**/*.ts && eslint spec/tests/**/*.ts",
    "checks": "npm run prettier && npm run lint && npm run tsc",
    "migrate-dev": "db-migrate --env dev up",
    "migrate-test": "db-migrate --env test up"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.2",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/pg": "^7.14.7",
    "@types/prettier": "^2.6.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
